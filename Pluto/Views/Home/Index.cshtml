    <script>
        function checkPassword() {
            var password = document.getElementById("password").value;
            var confirmPwd = document.getElementById("confirmPwd").value;

            if (confirmPwd == password) {
                document.getElementById("envoyer").hidden = false;
                document.getElementById("messadeVerification").innerText = null;
            }
            else {
                document.getElementById("messadeVerification").innerText = "le mot de passe et celui de la confirmation est différent";
                document.getElementById("envoyer").hidden = true;

            }


        }
        function activer()
        {
            document.getElementById("spinnerA").hidden = false

            document.getElementById("messageActivation").innerText = null;
            $.ajax({
                url: "/Home/activation",
                type: 'POST',
                data: { code: document.getElementById('code').value},
                success: function (reponse) {
                  

                    if (reponse.code == 200)
                    {
                        switch (reponse.result.role) {
                            case "Manager":
                                document.getElementById("spinnerA").hidden = true

                              window.location.href = "/Admins/Index"
                                break;
                            case "STUDENT":
                                document.getElementById("spinnerA").hidden = true

                                window.location.href = "/Etudiants/Index"
                                break;
                            case "PARENT":
                                document.getElementById("spinnerA").hidden = true

                                window.location.href = "/Parents/Index"
                                break;
                            default:
                                document.getElementById("spinnerA").hidden = true

                               window.location.href = "/Admins/Index"
                        }

                    }
                    else
                    {
                        document.getElementById("spinnerA").hidden = true

                        document.getElementById("messageActivation").innerText = reponse.message;
                    }

                }
                ,
                error: function (error) {
                    document.getElementById("spinnerA").hidden = false
                }


            });



        }
        function inscrire() {

            document.getElementById("messadeInscription").innerText = null
            document.getElementById("sms").innerText = null

            $.post('@Url.Action("Sinscrire")',

                {
                    nom: document.getElementById('nom').value,
                    prenom: document.getElementById('prenom').value,
                    adresse: document.getElementById('adresse').value,
                    email: document.getElementById('emailI').value,
                    phone: document.getElementById('phone').value,
                    pwd: document.getElementById('password').value
                },
            function (rep)
            {
                if (rep.code == 200)
                {


                    document.getElementById("messadeInscription").innerText = rep.message;
                    setTimeout(() => { document.getElementById('myModal2').style.display = 'none'; window.location.href = "/Admin/Index"}, 1000);
                }
                else
                {
                    document.getElementById("sms").innerText = rep.message;
                }
            }
            ).fail(function (xhr, status, error) {
                                //Ajax request failed.
                document.getElementById("sms").innerText = " une erreur interne est survenue";

                            });
        }
        function seConnecter()
        {
            document.getElementById("spinner").hidden = false

            document.getElementById("messadeConnexion").innerText = null;
            $.ajax({
                url: "/Home/Seconnecter",
                type: 'POST',
                data: { pwd: document.getElementById('pwd').value, phone: document.getElementById('phone').value },
                success: function (result)
                {


                    if (result.code == 200) {

                        document.getElementById('activation').style.display = 'block'
                        document.getElementById("spinner").hidden = true

                    }
                    else
                    {
                        document.getElementById("messadeConnexion").innerText = result.message;
                        document.getElementById("spinner").hidden = true

                    }

                },
                error: function (error)
                {
                    document.getElementById("spinner").hidden = false
                 }


            });


        }
        function isNumberKey2(evt)
        {
            var charCode = (evt.which) ? evt.which : event.keyCode;
            if (charCode != 46 && charCode > 31
                && (charCode < 48 || charCode > 57))
                return false;

            return true;
        }
