<script>
  
    function ajouterPersonnal() {
        document.getElementById("spinner").hidden = false
        document.getElementById("messadeAjouterPersonnal").innerText = null
        document.getElementById("smsAjouterPersonnal").innerText = null
        // Checking whether FormData is available in browser
        if (window.FormData !== undefined) {

            var fileUpload = $("#file").get(0);
            var files = fileUpload.files;
            var file = files[0]
            // Create FormData object

            var fileData = new FormData();
            fileData.append("file", file);
            fileData.append("prenom", $('#prenomProf').val());
            fileData.append("nom", $('#nomProf').val());
            fileData.append("naissance", $('#date_naissance').val());
            fileData.append("profession", $('#profession').val());
            fileData.append("email", $('#email').val());
            fileData.append("type_contrat", $('#type_contrat').val());
            fileData.append("telephone", $('#phone').val());
            fileData.append("adresse", $('#adresse').val());
            fileData.append("sexe", $('#sexe').val());
            fileData.append("nationalite", $('#nationalite').val());
            fileData.append("type", 1);
            fileData.append("compteBancaire", $('#compteBancaire').val());

            $.ajax({
                method: 'post',
                processData: false,
                contentType: false,
                cache: false,
                data: fileData,
                enctype: 'multipart/form-data',
                url: "/Admins/ajouterUser",
                success: function (reponse) {
                    if (reponse.code == 200) {
                        document.getElementById("spinner").hidden = true
                        document.getElementById("messadeAjouterPersonnal").innerText = reponse.message;
                        document.getElementById("smsAjouterPersonnal").innerText = null
                        setTimeout(() => { document.getElementById('mediumModalCreate').style.display = 'none'; window.location.reload() }, 1000);
                    }
                    else {
                        document.getElementById("spinner").hidden = true
                        document.getElementById("smsAjouterPersonnal").innerText = reponse.message;
                        document.getElementById("messadeAjouterPersonnal").innerText = null

                    }
                },
                error: function (reponse) {
                    document.getElementById("spinner").hidden = true
                    document.getElementById("smsAjouterPersonnal").innerText = " une erreur interne est survenue";
                    document.getElementById("messadeAjouterPersonnal").innerText = null
                }
            });

        } else {

            document.getElementById("spinner").hidden = true
            document.getElementById("smsAjouterPersonnal").innerText = "FormData is not supported.";
            document.getElementById("messadeAjouterPersonnal").innerText = null
        }

    };
</script>


