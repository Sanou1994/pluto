<script>

    function ajouterPreinscription() {
        document.getElementById("spinner").hidden = false
        document.getElementById("messadeAjouterEtudiant").innerText = null
        document.getElementById("smsAjouterEtudiant").innerText = null
        // Checking whether FormData is available in browser
        if (window.FormData !== undefined)
        {

            var photocopie_piece_identite = $("#photocopie_piece_identite").get(0).files;
            var photocopie_releve_notes = $("#photocopie_releve_notes").get(0).files;
            var photocopie_attestation = $("#photocopie_attestation").get(0).files;
            var photo_identite = $("#photo_identite").get(0).files;

            var filePhotocopie_piece_identite = photocopie_piece_identite[0]
            var filePahotocopie_releve_notes = photocopie_releve_notes[0]
            var filePhotocopie_attestation = photocopie_attestation[0]
            var filePhoto_identite = photo_identite[0]

            // Create FormData object

            var fileData = new FormData();
            

            fileData.append("photo_identite", filePhotocopie_piece_identite);
            fileData.append("photocopie_attestation", filePhotocopie_attestation);
            fileData.append("photocopie_releve_notes", filePahotocopie_releve_notes);
            fileData.append("photocopie_piece_identite", filePhoto_identite);
            fileData.append("id", $('#id').val()); 
            fileData.append("etat", $('#etat').val()); 
            fileData.append("type", $('#type').val());
            fileData.append("pays_de_naissance", $('#pays_de_naissance').val());
            fileData.append("pays_de_residence", $('#pays_de_residence').val());
            fileData.append("indicatif", $('#indicatif').val());
            fileData.append("dernier_etablissement_frequente", $('#dernier_etablissement_frequente').val());
            fileData.append("adresse_complete", $('#adresse_complete').val());
            fileData.append("dernier_diplome_obtenue", $('#dernier_diplome_obtenue').val());
            fileData.append("type_de_cours", $('#type_de_cours').val());
            fileData.append("programme_choisi", $('#programme_choisi').val());
            fileData.append("niveau", $('#niveau').val());
            fileData.append("specialite", $('#specialite').val());
            fileData.append("frais_dossier", $('#frais_dossier').val());
            fileData.append("responsable_financier", $('#responsable_financier').val());
            fileData.append("nom_responsable", $('#nom_responsable').val());
            fileData.append("profession_du_responsable", $('#profession_du_responsable').val());
            fileData.append("numéro_de_téléphone_du_responsable", $('#numéro_de_téléphone_du_responsable').val());
            fileData.append("adresse_email_responsable", $('#adresse_email_responsable').val());
            fileData.append("adresse_responsable", $('#adresse_responsable').val());
            fileData.append("autre_contact", $('#autre_contact').val());


            $.ajax({
                method: 'post',
                processData: false,
                contentType: false,
                cache: false,
                data: fileData,
                enctype: 'multipart/form-data',
                url: "/Admins/ajouterUser",
                success: function (reponse) {

                    if (reponse.code == 200) {
                        document.getElementById("spinner").hidden = true
                        document.getElementById("messadeAjouterEtudiant").innerText = reponse.message;
                        document.getElementById("smsAjouterEtudiant").innerText = null
                        setTimeout(() => { document.getElementById('mediumModalCreate').style.display = 'none'; window.location.reload() }, 1000);
                    }
                    else {
                        document.getElementById("spinner").hidden = true
                        document.getElementById("smsAjouterEtudiant").innerText = reponse.message;
                        document.getElementById("messadeAjouterEtudiant").innerText = null

                    }
                },
                error: function (reponse) {
                    document.getElementById("spinner").hidden = true
                    document.getElementById("smsAjouterEtudiant").innerText = " une erreur interne est survenue ";
                    document.getElementById("messadeAjouterEtudiant").innerText = null
                }
            });

        } else {

            document.getElementById("spinner").hidden = true
            document.getElementById("smsAjouterEtudiant").innerText = "FormData is not supported.";
            document.getElementById("messadeAjouterEtudiant").innerText = null
        }

    };
</script>
